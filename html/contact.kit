<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Forest group</title>
    <link rel="stylesheet" href="/dist/css/hamburger.min.css">
    <link rel="stylesheet" href="/dist/css/main.min.css">
    <script src="https://kit.fontawesome.com/a0eb9ef7d9.js" crossorigin="anonymous"></script>
    <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@200;400&display=swap" rel="stylesheet">
</head>
<body>

<!-- @include "nav.kit" -->

<main>

    <section class="contact">
            <h2 class="section-heading">Kontakt</h2>
            <div class="contact-box">
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2559.898451398873!2d19.890660217443834!3d50.0881881!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a326d015c3b%3A0x47a12cd367a1f34e!2sRestauracja%20McDonald&#39;s!5e0!3m2!1spl!2spl!4v1646487645487!5m2!1spl!2spl" width="50%" height="650" style="border:0;" allowfullscreen="" loading="lazy" class="contact-map"></iframe>

      <form action="" class="contact__form">

                    <div class="contact__form-box">
                        <label for="name">Your <span>name:</span></label>
                        <input type="text" id="name">
                    </div>
                    <div class="contact__form-box">
                        <label for="mail">Your <span>email:</span></label>
                        <input type="email" id="mail">
                    </div>
                    <div class="contact__form-box">
                        <label for="msg">Message</label>
                        <textarea id="msg" cols="30" rows="10"></textarea>
                    </div>

                    <button type="submit" class='contact__form-btn'>Send</button>
                </form>
            </div>
</section>

</main>

<!-- @include "footer.kit" -->



<script src="/dist/js/main.min.js"></script>
</body>
</html>
